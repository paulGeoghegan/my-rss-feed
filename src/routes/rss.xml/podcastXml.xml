		<language>en</language>
		<itunes:explicit>false</itunes:explicit>
		<pubDate>Mon, 29 Jan 2024 16:00:00 GMT</pubDate>
		<link>https://mywebsite.com/rss.xml</link>
		<itunes:image href="https://mywebsite.com/podcastLogo.jpeg" />
		<item>
			<guid>https://mywebsite.com/episode1.mp3</guid>
			<title>Episode 1, my episode name</title>
			<itunes:image href="https://mywebsite.com/podcastLogo.jpeg" />
			<description>myEpisodeDescription</description>
			<pubDate>2024-02-01</pubDate>
			<enclosure url="https://mywebsite.com/episode1.mp3" type="audio/mpeg" length="episodeLength" />
			<itunes:duration>episodeLength</itunes:duration>
			<itunes:season>1</itunes:season>
			<itunes:episode>1</itunes:episode>
			<itunes:keywords>my,key,words</itunes:keywords>
		</item>
	</channel>
</rss>